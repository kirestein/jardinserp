# Ambiente de Staging (Railway)
NODE_ENV=staging
PORT=3001

# Banco PostgreSQL Staging (Railway)
# STAGING_DATABASE_URL=postgresql://postgres:STAGING_PASSWORD@staging-host:porta/railway
# Será configurado no Railway Dashboard

# JWT Secret (diferente da produção)
JWT_SECRET=staging_jwt_secret_change_this_in_railway

# Configurações de Upload
MAX_FILE_SIZE=5242880
ALLOWED_FILE_TYPES=image/jpeg,image/png,image/gif

# Configurações de Segurança
CORS_ORIGIN=*

# Debug
DEBUG=false